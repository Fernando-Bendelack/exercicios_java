import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
         
            
        int distanc_km, result_min;
        
        distanc_km = sc.nextInt();

        result_min = distanc_km * 2;
        
        System.out.println(result_min + " minutos");
        
    
        sc.close();
    }
}
