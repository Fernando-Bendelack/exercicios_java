import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        
        int X;
        double Y;
        X = sc.nextInt();
        Y = sc.nextDouble();
        
        double consum_med = X / Y;
        
        System.out.printf("%.3f km/l\n", consum_med);   
        
        sc.close();
    }
}
